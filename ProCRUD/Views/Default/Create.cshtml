@model  ProCRUD.Models.Table

@{
    ViewBag.Title = "Create";
}

<h2>Create</h2>

<p>
    @Html.ActionLink("Back to Index", "Read")
</p>

@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()


    <div class="form-group">
        @Html.LabelFor(m => m.Account) <br>
        @Html.TextBoxFor(m => m.Account)
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.Password)<br>
        @Html.TextBoxFor(m => m.Password)
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.UserName) <br>
        @Html.TextBoxFor(m => m.UserName)      
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.Email)<br>
        @Html.TextBoxFor(m => m.Email)    
    </div>
    <div class="form-group">
        @Html.LabelFor(m => m.Address)<br>
        @Html.TextBoxFor(m => m.Address)
    </div>
      
   

    <div class="col-md-offset-2 col-md-10">
        <input type="submit" name="add" value="註冊" class="btn btn-primary">
    </div>



}
